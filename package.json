{
  "name": "homenet-core",
  "version": "4.0.0-alpha.21",
  "description": "Homenet home automation - core system",
  "main": "lib/index.js",
  "author": "Den Williams",
  "license": "MIT",
  "engines": {
    "node": ">=0.12"
  },
  "scripts": {
    "build": "tsc --sourceMap",
    "prelocaltest": "npm run build",
    "localtest": "ava -v",
    "coverage": "nyc ava -v",
    "jsdoc": "jsdoc -c .jsdoc",
    "generate-docs": "jsdoc --configure ./.jsdoc --verbose",
    "test": "ava -v",
    "watchtest": "tsc -w -v",
    "watchbuildtest": "tsc -w -v & ava -v -w",
    "tsdoc": "typedoc --verbose --mode file --out ./doc/ --module commonjs --target ES5 --exclude node_modules/* --includeDeclarations main.ts"
  },
  "node-red": {
    "nodes": {
      "current-scene": "node_modules/node-red-contrib-scenes/nodes/current-scene.js",
      "scene": "node_modules/node-red-contrib-scenes/nodes/scene.js",
      "scene-startstop": "node_modules/node-red-contrib-scenes/nodes/scene-startstop.js"
    },
    "disabled": {
      "app-event": "node_modules/homenet-nodes/nodes/app-event.js",
      "bump-presence": "node_modules/homenet-nodes/nodes/bump-presence.js",
      "currentlight": "node_modules/homenet-nodes/nodes/currentlight.js",
      "currentpresence": "node_modules/homenet-nodes/nodes/currentpresence.js",
      "dark": "node_modules/homenet-nodes/nodes/dark.js",
      "deserialize": "node_modules/homenet-nodes/nodes/deserialize.js",
      "device-sensor": "node_modules/homenet-nodes/nodes/device-sensor.js",
      "isdark": "node_modules/homenet-nodes/nodes/isdark.js",
      "islight": "node_modules/homenet-nodes/nodes/islight.js",
      "ispresent": "node_modules/homenet-nodes/nodes/ispresent.js",
      "isscene": "node_modules/homenet-nodes/nodes/isscene.js",
      "isweekday": "node_modules/homenet-nodes/nodes/isweekday.js",
      "isweekend": "node_modules/homenet-nodes/nodes/isweekend.js",
      "light": "node_modules/homenet-nodes/nodes/light.js",
      "lights": "node_modules/homenet-nodes/nodes/lights.js",
      "locks": "node_modules/homenet-nodes/nodes/locks.js",
      "media-state": "node_modules/homenet-nodes/nodes/media-state.js",
      "notify": "node_modules/homenet-nodes/nodes/notify.js",
      "presence-parent": "node_modules/homenet-nodes/nodes/presence-parent.js",
      "presence": "node_modules/homenet-nodes/nodes/presence.js",
      "remember": "node_modules/homenet-nodes/nodes/remember.js",
      "scheduler": "node_modules/homenet-nodes/nodes/scheduler.js",
      "secure": "node_modules/homenet-nodes/nodes/secure.js",
      "sensor": "node_modules/homenet-nodes/nodes/sensor.js",
      "timeofday": "node_modules/homenet-nodes/nodes/timeofday.js",
      "currentscene": "nodes/currentscene.js"
    }
  },
  "dependencies": {
    "body-parser": "^1.12.0",
    "chalk": "^1.1.1",
    "error-builder": "^1.1.0",
    "eventemitter2": "^0.4.14",
    "express": "^4.11.2",
    "inversify": "^2.0.0-rc.1",
    "lodash": "^4.12.0",
    "node-red-contrib-scenes": "^0.1.8",
    "q": "^1.4.1",
    "redis": "^2.5.3",
    "reflect-metadata": "^0.1.3",
    "source-map-support": "^0.4.0",
    "suncalc": "^1.7.0",
    "winston": "^2.2.0",
    "winston-papertrail": "^1.0.2"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "inversify-dts": "^1.4.0",
    "nyc": "^6.6.1",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.2.0"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denwilliams/homenet-core.git"
  },
  "bugs": {
    "url": "https://github.com/denwilliams/homenet-core/issues"
  },
  "homepage": "https://github.com/denwilliams/homenet-core#readme"
}
