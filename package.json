{
  "name": "homenet-core",
  "version": "4.0.0-alpha",
  "description": "Homenet home automation - core system",
  "main": "index.js",
  "scripts": {
    "prepublish": "tsc",
    "pretest": "tsc",
    "test": "mocha --require source-map-support/register --recursive build/test",
    "build": "tsc",
    "build-run": "npm run build && node build/main",
    "jsdoc": "jsdoc -c .jsdoc",
    "tsdoc": "typedoc --verbose --mode file --out ./doc/ --module commonjs --target ES5 --exclude node_modules/* --includeDeclarations ./lib",
    "generate-docs": "jsdoc --configure ./.jsdoc --verbose"
  },
  "author": "Den Williams",
  "license": "MIT",
  "dependencies": {
    "async-ioc": "^0.2.0",
    "body-parser": "^1.12.0",
    "chalk": "^1.1.1",
    "error-builder": "^1.1.0",
    "eventemitter2": "^0.4.14",
    "express": "^4.11.2",
    "inversify": "^1.2.2",
    "lodash": "^3.10.1",
    "moment": "^2.10.6",
    "net-ping": "^1.1.11",
    "node-hue-api": "^1.2.0",
    "node-milight-promise": "0.0.3",
    "node-red": "^0.10.0",
    "q": "^1.1.2",
    "redis": "~0.12.1",
    "socket.io": "^1.3.4",
    "suncalc": "^1.6.0",
    "typescript": "^1.7.5",
    "typescript-require": "^0.2.9",
    "underscore": "^1.8.1",
    "when": "^3.7.2",
    "winston": "^0.9.0",
    "winston-papertrail": "^1.0.1"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "gulp": "^3.9.0",
    "gulp-typedoc": "^1.2.1",
    "gulp-typescript": "^2.9.2",
    "ink-docstrap": "^0.5.2",
    "jsdoc": "^3.3.3",
    "loke-jsdoc-theme": "^2.1.0",
    "minami": "^1.1.1",
    "node-zway": "^0.1.0",
    "should": "^7.1.1",
    "socket.io-client": "^1.3.7",
    "typedoc": "^0.3.12"
  }
}
