{
  "name": "homenet-core",
  "version": "4.0.0-alpha.14",
  "description": "Homenet home automation - core system",
  "main": "lib/index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "prepublish": "tsc",
    "pretest": "tsc",
    "test": "mocha --require source-map-support/register --recursive build/test",
    "build": "tsc",
    "build-run": "npm run build && node build/main",
    "jsdoc": "jsdoc -c .jsdoc",
    "tsdoc": "typedoc --verbose --mode file --out ./doc/ --module commonjs --target ES5 --exclude node_modules/* --includeDeclarations main.ts",
    "generate-docs": "jsdoc --configure ./.jsdoc --verbose"
  },
  "author": "Den Williams",
  "license": "MIT",
  "node-red": {
    "nodes": {
      "app-event": "nodes/app-event.js",
      "bump-presence": "nodes/bump-presence.js",
      "currentlight": "nodes/currentlight.js",
      "currentpresence": "nodes/currentpresence.js",
      "dark": "nodes/dark.js",
      "deserialize": "nodes/deserialize.js",
      "device-sensor": "nodes/device-sensor.js",
      "isdark": "nodes/isdark.js",
      "islight": "nodes/islight.js",
      "ispresent": "nodes/ispresent.js",
      "isscene": "nodes/isscene.js",
      "isweekday": "nodes/isweekday.js",
      "isweekend": "nodes/isweekend.js",
      "light": "nodes/light.js",
      "lights": "nodes/lights.js",
      "locks": "nodes/locks.js",
      "media-state": "nodes/media-state.js",
      "notify": "nodes/notify.js",
      "presence-parent": "nodes/presence-parent.js",
      "presence": "nodes/presence.js",
      "remember": "nodes/remember.js",
      "scheduler": "nodes/scheduler.js",
      "secure": "nodes/secure.js",
      "sensor": "nodes/sensor.js",
      "timeofday": "nodes/timeofday.js",
      "current-scene": "node_modules/node-red-contrib-scenes/nodes/current-scene.js",
      "scene": "node_modules/node-red-contrib-scenes/nodes/scene.js",
      "scene-startstop": "node_modules/node-red-contrib-scenes/nodes/scene-startstop.js"
    },
    "disabled": {
      "currentscene": "nodes/currentscene.js"
    }
  },
  "dependencies": {
    "body-parser": "^1.12.0",
    "chalk": "^1.1.1",
    "error-builder": "^1.1.0",
    "eventemitter2": "^0.4.14",
    "express": "^4.11.2",
    "inversify": "^2.0.0-beta.6",
    "inversify-dts": "^1.0.0",
    "lodash": "^4.12.0",
    "moment": "^2.10.6",
    "net-ping": "^1.1.11",
    "node-hue-api": "^1.2.0",
    "node-milight-promise": "0.0.3",
    "node-red-contrib-scenes": "^0.1.8",
    "q": "^1.4.1",
    "redis": "^2.5.3",
    "reflect-metadata": "^0.1.3",
    "socket.io": "^1.4.6",
    "source-map-support": "^0.4.0",
    "suncalc": "^1.7.0",
    "winston": "^2.2.0",
    "winston-papertrail": "^1.0.2"
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "gulp": "^3.9.0",
    "gulp-typedoc": "^1.2.1",
    "gulp-typescript": "^2.9.2",
    "ink-docstrap": "^0.5.2",
    "jsdoc": "^3.3.3",
    "loke-jsdoc-theme": "^2.1.0",
    "minami": "^1.1.1",
    "node-zway": "^0.1.0",
    "should": "^7.1.1",
    "socket.io-client": "^1.3.7",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^0.6.8"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denwilliams/homenet-core.git"
  },
  "bugs": {
    "url": "https://github.com/denwilliams/homenet-core/issues"
  },
  "homepage": "https://github.com/denwilliams/homenet-core#readme"
}
