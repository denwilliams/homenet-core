{
  "name": "@homenet/core",
  "version": "4.0.0-beta.19",
  "description": "Homenet home automation - core system",
  "main": "lib/index.js",
  "author": "Den Williams",
  "license": "MIT",
  "engines": {
    "node": ">=4"
  },
  "bin": {
    "homenet4": "bin/homenet4"
  },
  "scripts": {
    "build": "tsc --sourceMap",
    "builddocs": "aglio --theme-variables streak --theme-template triple -i docs/api.apib -o docs/api.html",
    "prelocaltest": "npm run build",
    "localtest": "ava -v",
    "coverage": "nyc ava -v",
    "jsdoc": "jsdoc -c .jsdoc",
    "generate-docs": "jsdoc --configure ./.jsdoc --verbose",
    "test": "ava -v",
    "watchtest": "tsc -w -v",
    "watchbuildtest": "tsc -w -v & ava -v -w",
    "typedoc": "typedoc --mode file --out ./docs/out/tsdoc/ --module commonjs --target ES6 --exclude node_modules/* --includeDeclarations --excludeExternals tsconfig.json",
    "typedocv": "typedoc --verbose --mode file --out ./out/tsdoc/ --module commonjs --target ES6 --exclude node_modules/* --includeDeclarations tsconfig.json"
  },
  "node-red": {
    "nodes": {
      "current-scene": "node_modules/node-red-contrib-scenes/nodes/current-scene.js",
      "scene": "node_modules/node-red-contrib-scenes/nodes/scene.js",
      "scene-startstop": "node_modules/node-red-contrib-scenes/nodes/scene-startstop.js"
    }
  },
  "dependencies": {
    "@homenet/nodes": "^4.0.0-beta.8",
    "body-parser": "^1.12.0",
    "chalk": "^1.1.1",
    "cors": "^2.8.1",
    "error-builder": "^1.1.0",
    "eventemitter2": "^2.2.1",
    "express": "^4.11.2",
    "express-graphql": "^0.6.6",
    "graphql": "^0.10.1",
    "graphql-tools": "^1.0.0",
    "inversify": "3.1.0",
    "lodash": "^4.12.0",
    "module-finder": "^4.0.1",
    "node-red": "^0.16.2",
    "node-red-contrib-scenes": "^0.1.8",
    "q": "^1.4.1",
    "redis": "^2.5.3",
    "reflect-metadata": "^0.1.8",
    "source-map-support": "^0.4.6",
    "suncalc": "^1.7.0",
    "winston": "^2.2.0",
    "winston-papertrail": "^1.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.36",
    "@types/body-parser": "0.0.33",
    "@types/chalk": "^0.4.31",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.34",
    "@types/lodash": "^4.14.41",
    "@types/node": "^7.0.4",
    "@types/q": "0.0.32",
    "@types/redis": "^0.12.33",
    "@types/sinon": "^1.16.32",
    "@types/supertest-as-promised": "^2.0.32",
    "@types/when": "^2.4.28",
    "ava": "^0.17.0",
    "inversify-dts": "^3.0.0",
    "nyc": "^6.6.1",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.2.0"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denwilliams/homenet-core.git"
  },
  "bugs": {
    "url": "https://github.com/denwilliams/homenet-core/issues"
  },
  "homepage": "https://github.com/denwilliams/homenet-core#readme",
  "keywords": [
    "homenet",
    "homenet4",
    "homenet4-core"
  ]
}
